{
  "tsserver.npm": "/usr/local/bin/node",
  "coc.preferences.extensionUpdateCheck": "daily",
  "eslint.run": "onSave",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "json",
    "dart"
  ],
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "cSpell.userWords": [
    "Spotify"
  ]
}
